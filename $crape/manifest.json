{
    "manifest_version": 3,
    "name": "$crape",
    "version": "1.0",
    "description": "Analyze Amazon Product Reviews",
    "permissions": ["activeTab", "tabs", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}